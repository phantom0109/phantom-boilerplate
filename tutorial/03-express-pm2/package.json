{
  "name": "js-stack-from-scratch",
  "version": "2.0.0",
  "description": "JavaScript Stack from Scratch - Step-by-step tutorial to build a modern JavaScript stack",
  "scripts": {
    "start": "yarn dev",
    "dev": "yarn stop && pm2 start pm2-dev.yaml",
    "prod": "yarn stop && yarn build && pm2 start pm2-prod.yaml",
    "build": "rimraf lib && babel src -d lib",
    "stop": "rimraf logs/* && pm2 delete all || true",
    "test": "eslint src && flow",
    "precommit": "yarn test && yarn build",
    "prepush": "yarn test && yarn build"
  },
  "dependencies": {
    "express": "^4.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-latest": "^6.22.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.38.0",
    "husky": "^0.13.1",
    "pm2": "^2.3.0",
    "rimraf": "^2.5.4"
  },
  "repository": "verekia/js-stack-from-scratch",
  "author": "Jonathan Verrecchia - @verekia",
  "license": "MIT"
}
